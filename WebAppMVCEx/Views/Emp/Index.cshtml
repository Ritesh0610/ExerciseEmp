@model IEnumerable<WebAppMVCEx.Models.Emp>

@{
    ViewBag.Title = "Index";
}

@Html.Partial("OurPView")
<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EDesig)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EDOJ)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EDesig)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EDOJ)
        </td>
    </tr>
}

</table>
